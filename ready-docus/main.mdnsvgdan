    body,
    svg {
      height: 100%;
    }

html

    <svg viewBox="0 0 250 50" xmlns="http://www.w3.org/2000/svg">
      <text y="20" text-decoration="underline">Underlined text</text>
      <text x="0" y="40" text-decoration="line-through">Struck-through text</text>
    </svg>

