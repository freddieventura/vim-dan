      <defs>
        <linearGradient
          id="rainbow"
          gradientUnits="userSpaceOnUse"
          x1="0"
          y1="0"
          x2="100%"
          y2="0">
          <stop offset="0" stop-color="#ff0000"></stop>
          <stop offset="0.2" stop-color="#ffff00"></stop>
