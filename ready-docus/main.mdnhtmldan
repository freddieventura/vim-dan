    th {
      border: 1px solid rgb(160 160 160);
      padding: 8px 10px;
    }

    tr:nth-of-type(odd) td {
      background-color: #eee;
    }

    tr th[scope="row"] {
      background-color: #d6ecd4;
    }

    table {
      border-collapse: collapse;
